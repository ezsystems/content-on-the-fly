{
    "name": "ezsystems/content-on-the-fly-prototype",
    "description": "Platform UI Content on the Fly feature",
    "type": "ezplatform-bundle",
    "authors": [
        {
            "name": "eZ Systems",
            "email": "dev-team@ez.no"
        }
    ],
    "license": "GPL-2.0",
    "autoload": {
        "psr-4": {
            "EzSystems\\EzContentOnTheFly\\": "lib",
            "EzSystems\\EzContentOnTheFlyBundle\\": "bundle"
	}
    },
    "require": {
    	"ezsystems/platform-ui-bundle": "^1.4",
        "ezsystems/ezpublish-kernel": "^6.3"
